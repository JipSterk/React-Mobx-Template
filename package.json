{
    "name": "react-template",
    "version": "1.0.0",
    "private": true,
    "description": "A frontend boilerplate with React and Typescript",
    "main": "index.html",
    "scripts": {
        "start:dev": "cross-env NODE_ENV=development ts-node script/start",
        "start:prod": "cross-env NODE_ENV=production ts-node script/start",
        "build:dev": "cross-env NODE_ENV=development parallel-webpack --config src/webpack.development.ts",
        "build:prod": "cross-env NODE_ENV=production parallel-webpack --config src/webpack.production.ts"
    },
    "license": "MIT",
    "devDependencies": {
        "@types/clean-webpack-plugin": "^0.1.2",
        "@types/express": "^4.16.0",
        "@types/html-webpack-plugin": "^3.2.0",
        "@types/mini-css-extract-plugin": "^0.2.0",
        "@types/react": "^16.4.13",
        "@types/react-dom": "^16.0.7",
        "@types/webpack": "^4.4.11",
        "@types/webpack-dev-middleware": "^2.0.2",
        "@types/webpack-hot-middleware": "^2.16.4",
        "@types/webpack-merge": "^4.1.3",
        "awesome-typescript-loader": "^5.2.1",
        "clean-webpack-plugin": "^0.1.19",
        "cross-env": "^5.2.0",
        "css-loader": "^1.0.0",
        "express": "^4.16.3",
        "file-loader": "^2.0.0",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "mini-css-extract-plugin": "^0.4.2",
        "node-sass": "^4.9.3",
        "parallel-webpack": "^2.3.0",
        "resolve-url-loader": "^2.3.0",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.0",
        "to-string-loader": "^1.1.5",
        "ts-node": "^7.0.1",
        "typescript": "^3.0.3",
        "url-loader": "^1.1.1",
        "webpack": "^4.17.2",
        "webpack-dev-middleware": "^3.2.0",
        "webpack-hot-middleware": "^2.23.1",
        "webpack-merge": "^4.1.4"
    },
    "dependencies": {
        "mobx": "^5.1.0",
        "mobx-react": "^5.2.6",
        "react": "^16.5.0",
        "react-dom": "^16.5.0"
    }
}
