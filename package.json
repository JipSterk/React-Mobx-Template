{
    "name": "react-template",
    "version": "1.0.0",
    "private": true,
    "description": "A frontend boilerplate with React and Typescript",
    "main": "index.html",
    "scripts": {
        "start:dev": "cross-env NODE_ENV=development ts-node script/start",
        "start:prod": "cross-env NODE_ENV=production ts-node script/start",
        "build:dev": "cross-env NODE_ENV=development parallel-webpack --config src/webpack.development.ts",
        "build:prod": "cross-env NODE_ENV=production parallel-webpack --config src/webpack.production.ts"
    },
    "license": "MIT",
    "devDependencies": {
        "@types/clean-webpack-plugin": "^0.1.2",
        "@types/express": "^4.16.0",
        "@types/extract-text-webpack-plugin": "^3.0.3",
        "@types/html-webpack-plugin": "^2.30.3",
        "@types/react": "^16.3.17",
        "@types/react-dom": "^16.0.6",
        "@types/webpack": "^4.4.0",
        "@types/webpack-dev-middleware": "^2.0.1",
        "@types/webpack-hot-middleware": "^2.16.3",
        "@types/webpack-merge": "^4.1.3",
        "awesome-typescript-loader": "^5.0.0",
        "clean-webpack-plugin": "^0.1.19",
        "cross-env": "^5.1.6",
        "css-loader": "^0.28.11",
        "express": "^4.16.3",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^1.1.11",
        "html-loader": "^0.5.5",
        "html-webpack-plugin": "^3.2.0",
        "node-sass": "^4.9.0",
        "parallel-webpack": "^2.3.0",
        "resolve-url-loader": "^2.3.0",
        "sass-loader": "^7.0.3",
        "style-loader": "^0.21.0",
        "to-string-loader": "^1.1.5",
        "ts-node": "^6.1.0",
        "typescript": "^2.9.1",
        "url-loader": "^1.0.1",
        "webpack": "^4.12.0",
        "webpack-dev-middleware": "^3.1.3",
        "webpack-hot-middleware": "^2.22.2",
        "webpack-merge": "^4.1.1"
    },
    "dependencies": {
        "mobx": "^5.0.0",
        "mobx-react": "^5.2.1",
        "react": "^16.4.0",
        "react-dom": "^16.4.0"
    }
}
