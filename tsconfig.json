{
    "compilerOptions": {
        "sourceMap": true,
        "target": "es5",
        "jsx": "react",
        "module": "commonjs",
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noImplicitReturns": false,
        "removeComments": true,
        "strictNullChecks": false,
        "outDir": "dist",
        "lib": [
            "es6",
            "es7",
            "dom"
        ]
    },
    "exclude": [
        "./dist",
        "./node_modules"
    ]
}